        "children": [
          {
            "_key": "2d7e5d5175080",
            "_type": "span",
            "marks": [],
            "text": "Art must be available to everyone."
          }
        ],
        "markDefs": [],
        "style": "h1"
      },
      {
        "_key": "699e721e09f4",
        "_type": "block",

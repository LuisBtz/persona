          {
            "_key": "df284c92b5e50",
            "_type": "span",
            "marks": [],
            "text": "Our Ethos"
          }
        ],
        "markDefs": [],
        "style": "h2"
      },
      {
        "_key": "506a8ec82f1b",
        "_type": "block",
        "children": [
